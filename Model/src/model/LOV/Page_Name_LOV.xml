<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="Page_Name_LOV"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  SelectList="TblPages.PAGES_ID, 
       TblPages.PAGES_NAME, 
       TblPages.AST"
  FromList="TBL_PAGES TblPages"
  Where="AST='Y'"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.LOV.Page_Name_LOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <EntityUsage
    Name="TblPages"
    Entity="model.EO.TblPages"/>
  <ViewAttribute
    Name="PagesId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="PagesId"
    EntityUsage="TblPages"
    AliasName="PAGES_ID"/>
  <ViewAttribute
    Name="PagesName"
    IsUnique="true"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="PagesName"
    EntityUsage="TblPages"
    AliasName="PAGES_NAME"/>
  <ViewAttribute
    Name="Ast"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Ast"
    EntityUsage="TblPages"
    AliasName="AST"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
