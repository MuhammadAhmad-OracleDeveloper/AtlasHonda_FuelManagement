  <SQLQuery><![CDATA[select TBL_FMRN_M.FMRN_M_ID,TBL_FMRN_M.VENDOR_ID,Tbl_FMVendors.FMVENDORS_NAME,TBL_FMRN_M.GROSS_WEIGHT,TBL_FMRN_M.TEAR_WEIGHT,TBL_FMRN_M.NET_WEIGHT,TBL_FMRN_M.FMRN_M_REC_DATE,TBL_FMRN_M.CREDIT_DAYS,TBL_FMRN_M.FMRN_M_REC_DATE+TBL_FMRN_M.CREDIT_DAYS Due_Date,Sum(TBL_FMRN_D.FMRN_NET_AMT) FMRN_NET_AMT 
from TBL_FMRN_M,TBL_FMRN_D,Tbl_FMVendors 
where 
TBL_FMRN_M.FMRN_M_ID = TBL_FMRN_D.FMRN_M_ID 
And 
Tbl_FMVendors.FMVENDORS_ID=TBL_FMRN_M.VENDOR_ID 
And 
TBL_FMRN_M.FMRN_M_ID not in(Select FMRN_M_ID from TBL_FMPDC_D_D) 
Group By TBL_FMRN_M.FMRN_M_ID,TBL_FMRN_M.VENDOR_ID,TBL_FMRN_M.FMRN_M_REC_DATE,CREDIT_DAYS,Tbl_FMVendors.FMVENDORS_NAME,TBL_FMRN_M.GROSS_WEIGHT,TBL_FMRN_M.TEAR_WEIGHT,TBL_FMRN_M.NET_WEIGHT 
Order By TBL_FMRN_M.FMRN_M_REC_DATE+TBL_FMRN_M.CREDIT_DAYS]]></SQLQuery>
